{
  "name": "idance-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "iDance - Social platform for dancers",
  "scripts": {
    "dev": "pnpm --filter @idance/backoffice dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "setup:test-user": "node packages/auth/src/scripts/create-test-user.js",
    "setup:test-user:dev": "node packages/auth/src/scripts/create-test-user.js .env.local",
    "setup:test-user:staging": "node packages/auth/src/scripts/create-test-user.js .env.staging",
    "setup:test-user:prod": "node packages/auth/src/scripts/create-test-user.js .env.production",
    "ci:setup-test-user": "node packages/auth/src/scripts/create-test-user.js .env"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.10.0"
}